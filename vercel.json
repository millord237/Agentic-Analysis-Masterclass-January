{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "OPENANALYST_API_KEY": "@openanalyst-api-key",
    "PERPLEXITY_API_KEY": "@perplexity-api-key"
  }
}
